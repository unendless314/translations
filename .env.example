# API Keys for LLM providers
# Copy this file to .env and fill in your actual keys

# Google Gemini API (for topic analysis and translation)
# Get your key at: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API (for A/B testing and comparison)
# Get your key at: https://platform.openai.com/api-keys
# Supported models: gpt-5-mini, gpt-5, o1-preview, o1-mini
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude API (alternative provider)
# Get your key at: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Set default model provider (gemini, openai, anthropic)
DEFAULT_LLM_PROVIDER=gemini
